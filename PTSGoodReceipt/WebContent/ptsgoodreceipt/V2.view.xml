            labelSpanL="3"
            labelSpanM="3"
            emptySpanL="4"
            emptySpanM="4"
            columnsL="2"
            columnsM="2">
            <f:content>
            
                <Label text="SNo" />
                <Input id = "SNo"/>
                           
                <Label text="Title" />
                <Input id = "Title"/>
                
                <Label text="Course" />
