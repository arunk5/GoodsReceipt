	<Page title="PTS Goods Receipt" showNavButton="true" navButtonPress="BackButtonPress">
		
	
	<f:SimpleForm id="SimpleFormDisplay354"
            minWidth="1024"
            maxContainerCols="2"
            editable="false"
            layout="ResponsiveGridLayout"
